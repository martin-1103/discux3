{
  "name": "discux3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/cleanup-logs.js && tsx server.ts",
    "build": "node scripts/cleanup-logs.js && next build",
    "start": "tsx server.ts",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "node scripts/run-all-tests.js",
    "test:watch": "jest --watch",
    "test:discussion": "tsx scripts/test-discussion-system.ts",
    "test:brutal": "tsx scripts/test-brutal-prompts.ts",
    "test:socket": "tsx scripts/test-socket-integration.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "migrate:vectors": "tsx scripts/migrate-vector-ids.ts",
    "clean-logs": "node scripts/cleanup-logs.js",
    "logs": "dir logs || echo 'No logs directory found'",
    "view-logs": "powershell -Command \"Get-Content logs\\app.log -Wait -Tail 10 2>$null || echo 'No app logs found'\"",
    "view-errors": "powershell -Command \"Get-Content logs\\errors.log -Wait -Tail 10 2>$null || echo 'No error logs found'\"",
    "view-errors-json": "powershell -Command \"try { Get-Content logs\\errors.log -Tail 10 | ConvertFrom-Json | Format-Table timestamp, level, message, category -AutoSize } catch { Get-Content logs\\errors.log -Tail 10 }\"",
    "view-warnings": "powershell -Command \"Get-Content logs\\warnings-*.log -Wait -Tail 10 2>$null || echo 'No warning logs found'\"",
    "view-db": "powershell -Command \"Get-Content logs\\database-*.log -Wait -Tail 10 2>$null || echo 'No database logs found'\"",
    "view-console": "powershell -Command \"Get-Content logs\\console-*.log -Wait -Tail 10 2>$null || echo 'No console logs found'\"",
    "all-logs": "echo '=== All Log Files ===' && dir logs || echo 'No logs directory'"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.7",
    "@hookform/resolvers": "^3.7.0",
    "@prisma/client": "^5.16.2",
    "@qdrant/js-client-rest": "^1.15.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucia": "^3.2.2",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.3",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "kill-port": "^2.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.16.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.0.0"
  }
}
