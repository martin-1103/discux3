# Database Configuration
DATABASE_URL="mysql://root:@localhost:3306/discux3"

# Authentication
# IMPORTANT: Generate your own secrets with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="generate-your-own-secret-here-use-command-above"

# JWT Configuration
# IMPORTANT: Generate your own secret with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET="generate-your-own-secret-here-use-command-above"

# Server Configuration
PORT=3000
NODE_ENV=development

# Z.AI API Configuration
# Get your API key from: https://z.ai/manage-apikey/apikey-list (free tier available)
ZAI_API_KEY=""
ZAI_API_URL="https://api.z.ai/api/paas/v4/chat/completions"

# Z.AI Model Configuration
# Available models: glm-4.6, glm-4.5, glm-4.5-flash, glm-4.5-air, glm-4.5-x
ZAI_MODEL="glm-4.5-flash"

# WebSocket Configuration
SOCKET_URL="http://localhost:3001"

# Qdrant Vector Database (Required for Phase 7 - Agent Memory)
# Local: Use Docker - docker run -p 6333:6333 qdrant/qdrant
# Cloud: Get free tier at https://qdrant.tech/
QDRANT_URL="http://localhost:6333"
QDRANT_API_KEY=""
QDRANT_COLLECTION="discux3_conversations"

# Google Gemini Embeddings (Required for Phase 7)
# Get your API key from: https://aistudio.google.com/apikey (free tier available)
GOOGLE_API_KEY="AIzaSyC_ZYuztD1bk9uQce0a1d1qAl0X_02_FDI"
GOOGLE_EMBEDDING_MODEL="text-embedding-004"

# Vector Database Configuration
VECTOR_DB_ENABLED=true
